{
  "manifest_version": 2,
  "version": "0.1",
  "name": "Quicksend",
  "description": "never have to answer trivial questions during class again",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://meet.google.com/*"
      ],
      "js": [
        "app.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
