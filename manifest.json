{
  "manifest_version": 2,
  "version": "69.0",
  "name": "Quicksend",
  "description": "The most essential tool for online school",
  "content_scripts": [
    {
      "matches": [
        "https://meet.google.com/*"
      ],
      "js": [
        "js/app.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "graphics/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "graphics/icon48.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage"
  ]
}
